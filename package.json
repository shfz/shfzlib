{
  "name": "shfzlib",
  "version": "0.0.1",
  "description": "Scenario-based fuzzing test execution tool's scenario library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist ; tsc",
    "test": "npm run build ; tsc examples/index.ts ; node examples/index.js",
    "lint": "eslint --ext \".ts\" --ignore-path .gitignore ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shfz/shfzlib.git"
  },
  "author": "hi120ki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shfz/shfzlib/issues"
  },
  "homepage": "https://github.com/shfz/shfzlib#readme",
  "lint-staged": {
    "*.{ts}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "axios": "^0.24.0",
    "axios-cookiejar-support": "^2.0.3",
    "cheerio": "^1.0.0-rc.10",
    "lodash": "^4.17.21",
    "tough-cookie": "^4.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.8",
    "@types/tough-cookie": "^4.0.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  }
}
